<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Links in Homepage">

	<listeners>
		<listener class-name="extentreportlistener.Extentreportlistener" />
	</listeners>

	<parameter name="homeUrl" value="https://www.ticketswap.com/"></parameter>

	<test thread-count="5" name="Browse Events">
		<classes>
			<class name="testpackage.Testhomepagelinks" />
		</classes>
	</test>
	
</suite>
